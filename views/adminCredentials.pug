extends layout 
block suite 
    main 
        nav.admin-credentials 
            a(href="/user/page") Dashboard 
        section.admin-credentials-body
            h1  Admin Name and Password 
            form(action="" method="POST") 
                .form-item  
                    label(for="adminName") Admin Name
                    input(type="text" required autofocus  name="adminName" id="adminName" autoComplete="on") 
                .form-item 
                    label(for="adminPass") Admin Password 
                    input(type="password" name="adminPass" required  id="adminPass")
                ul 
                    if error 
                        li(style="color:#d55")= error  
                button.form-submit(type="submit") Log In